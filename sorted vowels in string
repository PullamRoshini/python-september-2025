class Solution:
    def sortVowels(self, s: str) -> str:
        vowels="aeiouAEIOU"
        sorted_vowels=sorted([ch for ch in s if ch in vowels])
        result=[]
        j=0
        for ch in s:
            if ch in vowels:
                result.append(sorted_vowels[j])
                j+=1
            else:
                result.append(ch)
        return"".join(result)
        
