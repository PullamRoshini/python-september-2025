class Solution:
    def numberOfPairs(self, points: List[List[int]]) -> int:
        points.sort(key = lambda p: (p[0],-p[1]))
        ans = 0
        for i in range(len(points)):
            _,yi = points[i]
            maxY = float('-inf')
            for j in range(i+1,len(points)):
                _,yj = points[j]
                if yi >= yj > maxY:
                    ans += 1
                    maxY  = yj
        return ans
